{
  "name": "prisma-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run db:init &&   ts-node-dev --respawn --transpile-only --exit-child src/server.ts",
    "db:init": "npx prisma db push ",
    "db:migrate": "npx prisma migrate dev && npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "ts-node prisma/jobSeed.ts"
  }
,  
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ]
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "i": "^0.3.7",
    "npm": "^10.5.2",
    "prisma": "^5.12.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2"
  }
}